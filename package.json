{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@antv/l7": "^2.2.28",
    "@antv/l7-maps": "^2.2.28",
    "@mapbox/mapbox-gl-language": "^0.10.1",
    "@turf/turf": "^5.1.6",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.14",
    "classnames": "^2.2.6",
    "deck.gl": "^8.2.5",
    "lint-staged": "^10.0.7",
    "mapbox-gl": "^1.11.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-map-gl": "^5.2.7",
    "umi": "^3.2.14",
    "yorkie": "^2.0.0"
  }
}
